{
    "name": "@mangelmaxime/eleventy-layout-nacara",
    "private": false,
    "version": "0.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "test": "npx tsc && cd tests && npx ava",
        "test-watch-ava": "cd tests && npx ava --watch",
        "test-watch-typescript": "npx tsc --watch",
        "test-watch": "npx concurrently --handle-input -p none npm:test-watch-*",
        "demo": "cd docs && npx @11ty/eleventy",
        "docs-watch-typescript": "npx tsc --watch",
        "docs-watch-eleventy": "npx nodemon",
        "docs-watch": "npx shx rm -rf docs/_site && npx concurrently -p none npm:docs-watch-*",
        "prepublish": "npx tsc"
    },
    "keywords": [
        "eleventy",
        "eleventy-plugin",
        "11ty"
    ],
    "author": "Mangel Maxime",
    "license": "MIT",
    "devDependencies": {
        "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
        "@types/cross-spawn": "^6.0.2",
        "@types/fs-extra": "^11.0.1",
        "@types/luxon": "^3.3.0",
        "@types/node": "^18.15.11",
        "@types/prettier": "^2.7.2",
        "ava": "^5.2.0",
        "chalk": "^5.2.0",
        "changelog-parser": "^3.0.1",
        "concurrently": "^8.0.1",
        "debug": "^4.3.4",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.7",
        "primjs": "^0.6.5",
        "shx": "^0.3.4",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "@11ty/eleventy": "2.0.1",
        "@svgdotjs/svg.js": "^3.1.2",
        "bulma": "^0.9.4",
        "cross-spawn": "^7.0.3",
        "dayjs": "^1.11.7",
        "eleventy-sass": "^2.2.1",
        "fs-extra": "^11.1.1",
        "gatsby-remark-vscode": "^3.3.1",
        "lucide-static": "^0.137.0",
        "nano-jsx": "^0.0.37",
        "sass": "^1.61.0",
        "simple-icons": "^8.9.0",
        "svgdom": "^0.1.13"
    },
    "ava": {
        "extensions": [
            "ts",
            "tsx"
        ],
        "require": [
            "ts-node/register"
        ],
        "files": [
            "tests/**/*.ts",
            "!**/*.d.ts",
            "!**/_site/**"
        ],
        "ignoredByWatcher": [
            "tests/fixtures/**/_includes/nacara/**",
            "tests/fixtures/**/assets/nacara/**",
            "tests/fixtures/temp/**"
        ]
    }
}
